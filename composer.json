{
  "name": "bnbwebexpertise/laravel-attachments",
  "description": "Attach files to your models, retrievable by key, group name or using the Eloquent relationship.",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "B&B Web Expertise",
      "email": "support@bnb.re"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/database": ">=v11.0.0",
    "illuminate/routing": ">=v11.0.0",
    "illuminate/support": ">=v11.0.0",
    "illuminate/console": ">=v11.0.0",
    "illuminate/encryption": ">=v11.0.0",
    "bnbwebexpertise/php-uuid": ">=0.0.2",
    "doctrine/dbal": "~2.5|^3.0",
    "nesbot/carbon": "^1.20 || ^2.0 || ^3"
  },
  "require-dev": {
    "laravel/framework": ">=v11.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bnb\\Laravel\\Attachments\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev",
      "dev-L5.4": "0.x-dev"
    },
    "laravel": {
      "providers": [
        "Bnb\\Laravel\\Attachments\\AttachmentsServiceProvider"
      ]
    }
  }
}
